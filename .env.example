# Core runtime
AETHERNET_HOME=/var/lib/aethernet
AETHERNET_NAME=aethernet-agent
AETHERNET_GENESIS_PROMPT=You are a wallet-native sovereign agent.
AETHERNET_CREATOR_ADDRESS=0x0000000000000000000000000000000000000000
AETHERNET_CHAIN=eip155:8453
AETHERNET_API_PORT=4123
AETHERNET_DEBUG=false

# Provider contract
AETHERNET_PROVIDER=selfhost
AETHERNET_PROVIDER_API_URL=https://api.provider.local
AETHERNET_PROVIDER_API_KEY=

# Brain (OpenAI-first)
AETHERNET_OPENAI_API_KEY=
AETHERNET_OPENAI_API_URL=https://api.openai.com/v1/responses
AETHERNET_BRAIN_MODEL=gpt-4.1-mini
AETHERNET_BRAIN_API_KEY_ENV=AETHERNET_OPENAI_API_KEY
AETHERNET_BRAIN_TEMPERATURE=0.2
AETHERNET_BRAIN_MAX_OUTPUT_TOKENS=1200
AETHERNET_BRAIN_TIMEOUT_MS=25000
AETHERNET_BRAIN_MAX_RETRIES=2
AETHERNET_BRAIN_RETRY_BACKOFF_MS=600

# Wallet
AETHERNET_WALLET_PASSPHRASE=
AETHERNET_WALLET_SESSION_TTL_SEC=900

# Messaging + x402
AETHERNET_XMTP_ENABLED=true
AETHERNET_XMTP_ENV=dev
AETHERNET_XMTP_DB_PATH=
AETHERNET_X402_VERIFY_URL=
AETHERNET_X402_SETTLE_URL=

# Autonomy + safety
AETHERNET_AUTONOMY_INTERVAL_MS=30000
AETHERNET_AUTONOMY_MAX_ACTIONS=6
AETHERNET_AUTONOMY_MAX_ERRORS=5
AETHERNET_AUTONOMY_MAX_SLEEP_MS=300000
AETHERNET_AUTONOMY_MAX_BRAIN_FAILURES=5
AETHERNET_AUTONOMY_STRICT_ACTION_ALLOWLIST=true
AETHERNET_AUTONOMY_ALLOW_SELF_MOD_ACTION=false
AETHERNET_AGGRESSIVE_SELF_MOD=true

# Alerting + telemetry routing
AETHERNET_ALERTS_ENABLED=true
AETHERNET_ALERT_ROUTE=db
AETHERNET_ALERT_WEBHOOK_URL=
AETHERNET_ALERT_CRITICAL_THRESHOLD=1
AETHERNET_ALERT_BRAIN_FAILURE_THRESHOLD=3
AETHERNET_ALERT_QUEUE_DEPTH_THRESHOLD=100
AETHERNET_ALERT_WINDOW_MINUTES=10

# Tool sources (disabled by default)
AETHERNET_ALLOW_EXTERNAL_TOOLS=false

# Survival thresholds
AETHERNET_LOW_COMPUTE_USD=10
AETHERNET_CRITICAL_USD=2
AETHERNET_DEAD_USD=0
AETHERNET_ESTIMATED_USD_BALANCE=100

# Kubernetes provider options (if provider=kubernetes)
AETHERNET_K8S_NAMESPACE=
AETHERNET_K8S_IMAGE=
AETHERNET_K8S_COMMAND=
AETHERNET_K8S_CONTEXT=
AETHERNET_K8S_KUBECONFIG=
AETHERNET_K8S_POD_PREFIX=aethernet-sandbox
AETHERNET_K8S_EXEC_TIMEOUT_MS=30000

# Selfhost provider options (if provider=selfhost)
AETHERNET_SANDBOX_ROOT_DIR=
